<core:FragmentDefinition xmlns:core="sap.ui.core" controllerName="com.baba.ZMM_PALLET_RPT.controller.Dialog1" xmlns="sap.m">
    <Dialog horizontalScrolling="true" icon="sap-icon://system-exit" showHeader="true" state="Warning" stretch="false" title="Close Pallet History App" type="Standard" verticalScrolling="true">
        <content>
            <Text text="Do you want to close the Pallet History App ?" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
        </content>
        <beginButton/>
        <buttons>
            <Button text="Exit" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
            <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress1"/>
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</core:FragmentDefinition>
